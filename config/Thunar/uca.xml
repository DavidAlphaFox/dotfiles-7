<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
	<icon>xterm-color</icon>
	<name>Open Terminal Here</name>
	<unique-id>1430238653835641-3</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description>Example for a custom action</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>terminal</icon>
	<name>Open Command Prompt Here</name>
	<unique-id>1430238624398252-2</unique-id>
	<command>cygstart -- cmd.exe /K cd `cygpath -w %f`</command>
	<description>run windows cmd.exe</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>wine</icon>
	<name>Open with Windows</name>
	<unique-id>1430238533137667-1</unique-id>
	<command>cygstart -- %F</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>vim</icon>
	<name>Open with Vim</name>
	<unique-id>1429813148236127-2</unique-id>
	<command>gvim %F</command>
	<description></description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>file-roller</icon>
	<name>Extract To...</name>
	<unique-id>1428950660946004-1</unique-id>
	<command>file-roller -f %F</command>
	<description></description>
	<patterns>*.xz;*.tar;*.bz2;*.gz;*.zip;*.lzma;*.7z</patterns>
	<other-files/>
</action>
<action>
	<icon>file-roller</icon>
	<name>Create Archive...</name>
	<unique-id>1428366523382071-1</unique-id>
	<command>file-roller -d %F</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
</actions>
