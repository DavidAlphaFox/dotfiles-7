! URXVT
URxvt.font: xft:DejaVu Sans Mono:pixelsize=18,xft:Font Awesome 5 Free Solid:pixelsize=18
URxvt.letterSpace: -1
URxvt.boldFont:
URxvt.internalBorder: 8
URxvt.scrollBar: False
URxvt.cursorUnderline: True
URxvt.cursorBlink: True

! XFT
Xft.antialias: 1
Xft.autohint: 1
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.rgba: rgb

! COLORS
! special
*.foreground:   #bfbfbf
*.background:   #212121
*.cursorColor:  #bfbfbf

! black
*.color0:       #303030
*.color8:       #505050

! red
*.color1:       #804040
*.color9:       #99734d

! green
*.color2:       #408040
*.color10:      #4d9966

! yellow
*.color3:       #808040
*.color11:      #b3b359

! blue
*.color4:       #405580
*.color12:      #4d7f99

! magenta
*.color5:       #755199
*.color13:      #994d99

! cyan
*.color6:       #418c8c
*.color14:      #59b3b3

! white
*.color7:       #808080
*.color15:      #bfbfbf

